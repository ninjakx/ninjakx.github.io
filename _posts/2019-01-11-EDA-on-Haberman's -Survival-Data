        \PY{n}{class\PYZus{}2} \PY{o}{=} \PY{n}{hab}\PY{o}{.}\PY{n}{loc}\PY{p}{[}\PY{n}{hab}\PY{p}{[}\PY{l+s+s2}{\PYZdq{}}\PY{l+s+s2}{Survival\PYZus{}status}\PY{l+s+s2}{\PYZdq{}}\PY{p}{]} \PY{o}{==} \PY{l+m+mi}{2}\PY{p}{]}\PY{p}{;}
